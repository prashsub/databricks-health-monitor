  "benchmarks": {
    "questions": [
      {
        "id": "e5f431c4c4b44d11b54995799e36ea9e",
        "question": [
          "What is our job success rate this week?"
        ],
        "answer": [
          {
            "format": "SQL",
            "content": [
              "SELECT MEASURE(success_rate) as success_rate_pct FROM mv_job_performance WHERE run_date >= CURRENT_DATE() - INTERVAL 7 DAYS;"
            ]
          }
        ]
